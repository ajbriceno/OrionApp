@model IEnumerable<OrionApp.Models.Customer>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CustDocType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustDocNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustFirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustLastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustPhone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustAddress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustCity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustState)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustZipCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CustCountry)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UpdatedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CustDocType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustDocNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustFirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustLastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustPhone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustCity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustState)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustZipCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CustCountry)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdatedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
